// dot -Tps imm_stats.dot -o imm_stats.ps
digraph instr_supprt {
    
      
  T [label="Total\n3868"]
    SYS         [style=dashed; label="System\n203"] 
    CRYPTO      [style=dashed; label="Crypto\n35"] 
    X87         [style=dashed; label="X87\n155"] 
    BASE        [style=filled; label="Base\n51"] 
