    MEM_UG [label="UnGeneralized\n#182"]

    MEM -> MEM_G
    MEM -> MEM_UG

    {rank = same; MEM_UG; MEM_G; }
  }
  /***************************************************/

 /***************************************************/
 // Immediates
  subgraph cluster_2 {
    label =  "Immediate";
    style=filled;
    color = cornsilk;

    G [label="Generalized from Regs\nwo/w extend: 107/83\n#190"] 
    S1 [sides=5; shape=polygon; label="Stratified\n#28"] 
    //S [label="Strata\n106"] 
