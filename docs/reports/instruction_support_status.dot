    REGS_UNSTRATA -> REGS_UNSTRATA_STOKED
    REGS_UNSTRATA -> REGS_UGUSUS

    {rank = same; REGS_STRATA; REGS_UNSTRATA; }
    {rank = same; REGS_UNSTRATA_STOKED; REGS_UGUSUS; }
  }
  /***************************************************/

  /***************************************************/
  // Memory
  subgraph cluster_1 {
    label =  "Memory"
    style=filled;
    color = lightpink;

